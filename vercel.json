{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "api/app.py" },
      { "src": "/(.*)", "dest": "/static/index.html" }
    ]
  }
  